---
import BaseHead from '../components/Head/BaseHead.astro';
import Nav from '../components/Nav.astro';
import HomeHeader from '../components/HomeHeader.astro';
import Footer from '../components/Footer/Footer.astro'
import PrivacyPolicyText from '../components/PrivacyPolicyText.astro';

let pageTitle = 'Privacy Policy';
let pageDescription = '';
let seoTitle = 'Privacy Policy | Shooting Star Eats';
let seoDescription = 'Privacy Policy';

---

<html lang="en">
  <head>
    <BaseHead title={seoTitle} description={seoDescription} />
  </head>

  <body class="bg-white text-black font-body leading-normal personality-casual">
    <Nav />
    <main class="py-12 lg:py-20">
      <article class="max-w-6xl mx-auto px-3" content>
        <HomeHeader title={pageTitle} description={pageDescription} />
        <PrivacyPolicyText  />
      </article>
    </main>
    <Footer />
  </body>
</html>

